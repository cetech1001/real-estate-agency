{
  "name": "real-estate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/bin/www",
    "dev": "ts-node-dev --respawn --transpileOnly src/bin/www",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "build": "tsc && yarn run copyFiles",
    "copyFiles": "yarn run copyViewFiles && yarn run copyAssetFiles",
    "copyViewFiles": "copyfiles -u 1 src/views/**/**/**/**/**/* dist/",
    "copyAssetFiles": "copyfiles -u 1 src/public/**/**/* dist/"
  },
  "dependencies": {
    "@types/bunyan": "^1.8.6",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.12",
    "connect-mongo": "^3.1.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "email-validator": "^2.0.4",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "http-errors": "~1.6.3",
    "mongoose": "^5.7.12",
    "morgan": "~1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-mongo": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/copyfiles": "^2.1.1",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^2.6.3",
    "@types/email-validator": "^1.0.6",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/http-errors": "^1.6.3",
    "@types/mongoose": "^5.5.32",
    "@types/morgan": "^1.7.37",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "copyfiles": "^2.1.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "prettier": "^1.19.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2"
  }
}
